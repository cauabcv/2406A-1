    }while(contador<= 10)
        */ 
    
    // for - para
    // 
    // Para cada item em um intervalo ou colection,uma detrminada ação será executada
    // estrutura:
    // 
    // for(vAuxiliar in intervalo ou colection){
    // ação
    // }
    // 
    // 
    
    
    for(pneu in 1..4){
        println("1 pneu")
    }
    
    for(contador im 0..3){
        println((contador +1)+"$pneu")
    }
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
